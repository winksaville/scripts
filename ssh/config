# Suppress when doing git operations on github:
#   X11 forwarding request failed on channel 0
Host github.com
  ForwardX11 no

# Allow X11 forwarding on all hosts so nvim clipboard works
Host *
  ForwardX11 yes
  ForwardX11Trusted yes
